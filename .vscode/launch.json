{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "C++ Launch",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/test/a.out",
      "cwd": "${workspaceFolder}",
      "targetArchitecture": "x64",
      "args": [
        "${workspaceFolder}/test/test.txt"
      ],
      "stopAtEntry": true,
      "setupCommands": [
        {
          "description": "为 gdb 启用整齐打印",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "linux": {
        "MIMode": "gdb",
        "miDebuggerPath": "/usr/bin/gdb"
      }
    },
    {
      "name": "C++ Launch(MacOS)",
      "type": "cppdbg",
      "request": "launch",
      "preLaunchTask": "CMake: build",
      "program": "${workspaceFolder}/build/SYSY-compiler",
      "cwd": "${workspaceFolder}",
      "targetArchitecture": "x64",
      "stopAtEntry": false,
      "MIMode": "lldb"
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": true,
      "cwd": "c:/Users/30478/vscode daima/Compiler",
      "program": "c:/Users/30478/vscode daima/Compiler/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}